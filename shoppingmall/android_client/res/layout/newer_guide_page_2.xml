<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:background="#ffffffff"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:layout_marginTop="0.0dip" xmlns:android="http://schemas.android.com/apk/res/android">
	<ImageView android:id="@+id/guide_instruction"
		android:paddingTop="0.0dip" android:layout_width="fill_parent"
		android:layout_height="wrap_content" android:layout_marginTop="0.0dip"
		android:scaleType="centerInside" android:adjustViewBounds="true"
		android:layout_alignParentTop="true" android:visibility="invisible"
		android:layout_centerInParent="true" />
	<ImageView android:layout_gravity="center" android:id="@+id/guide_content_image"
		android:focusableInTouchMode="false" android:layout_width="wrap_content"
		android:layout_height="fill_parent" android:src="@drawable/newcomer_guide_content_2"
		android:layout_weight="1.0" android:layout_below="@id/guide_instruction"
		android:layout_centerHorizontal="true" />
	<ImageView android:id="@+id/guide_pre" android:clickable="true"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/new_comer_guide_navi_left" android:scaleType="fitCenter"
		android:layout_alignLeft="@id/guide_instruction"
		android:layout_centerVertical="true" />
	<ImageView android:id="@+id/guide_next" android:clickable="true"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:src="@drawable/new_comer_guide_navi_right" android:scaleType="fitCenter"
		android:layout_alignRight="@id/guide_instruction"
		android:layout_centerVertical="true" />
</RelativeLayout>
